@model string

<td style="width: 150px">
    <div class="btn-group">
        <a asp-action="Edit" asp-controller="AdminUser" class="btn btn-primary" asp-route-id="@Model">
            <i class="far fa-edit"></i>
        </a>
        <a asp-action="Details" asp-controller="AdminUser" class="btn btn-success" asp-route-id="@Model">
            <i class="fas fa-list-ul"></i>
        </a>
        <a asp-action="Lockout" asp-controller="AdminUser" class="btn btn-danger" asp-route-id="@Model">
            <i class="fas fa-trash-alt"></i>
        </a>
    </div>
</td>