<!doctype html>
<html lang="en">
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Bootstrap CSS -->
    <link href="~/admin_css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/admin_css/style.css" rel="stylesheet" />
    <link href="~/admin_css/style1.css" rel="stylesheet" />
    <link href="~/admin_css/fontawesome-all.css" rel="stylesheet" />
    <link href="~/admin_css/chartist.css" rel="stylesheet" />
    <link href="~/admin_css/morris.css" rel="stylesheet" />
    <link href="~/admin_css/c3.css" rel="stylesheet" />
    <link href="~/admin_css/materialdesignicons.min.css" rel="stylesheet" />
    <link href="~/admin_css/flag-icon.min.css" rel="stylesheet" />
    <title>FreshShop Admin</title>
</head>
<body>
    <!-- ============================================================== -->
    <!-- main wrapper -->
    <!-- ============================================================== -->
    <div class="dashboard-main-wrapper">
        <!-- ============================================================== -->
        <!-- navbar -->
        <!-- ============================================================== -->
        <div class="dashboard-header">
            <nav class="navbar navbar-expand-lg bg-dark fixed-top">
                <a class="navbar-brand"  href="/admin/Dashboard/Index" style="color:burlywood">FreshShop</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse " id="navbarSupportedContent">
                    <ul class="navbar-nav ml-auto navbar-right-top">
                        <li class="nav-item">
                            <div id="custom-search" class="top-search-bar">
                                <input class="form-control" type="text" placeholder="Search..">
                            </div>
                        </li>

                        <li class="nav-item dropdown nav-user">
                            <a class="nav-link nav-user-img" href="#" id="navbarDropdownMenuLink2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><img src="~/images/IshikaJoshi_Photo.png" alt="" class="user-avatar-md rounded-circle"></a>
                            <div class="dropdown-menu dropdown-menu-right nav-user-dropdown" aria-labelledby="navbarDropdownMenuLink2">
                                <div class="nav-user-info">
                                    <h5 class="mb-0 text-white nav-user-name">Admin</h5>
                                </div>
                                <a class="dropdown-item" href="#"><i class="fas fa-user mr-2"></i>Account</a>
                                <a class="dropdown-item" href="#"><i class="fas fa-power-off mr-2"></i>Logout</a>
                            </div>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
        <!-- ============================================================== -->
        <!-- end navbar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- left sidebar -->
        <!-- ============================================================== -->
        <div class="nav-left-sidebar bg-dark">
            <div class="menu-list">
                <nav class="navbar navbar-expand-lg">
                    <a class="d-xl-none d-lg-none" href="/admin/Dashboard/Index">Dashboard</a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarNav">
                        <ul class="navbar-nav flex-column">
                            <li class="nav-item navbar-light">
                                <a class="nav-link" href="/admin/Dashboard/Index"  style="color:coral"><i class="fa fa-fw fa fa-id-badge"></i>Dashboard</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#" data-toggle="collapse" aria-expanded="false" data-target="#submenu-2" aria-controls="submenu-2"  style="color: coral"><i class="fa fa-fw fa fa-shopping-bag"></i>Manage Products</a>
                                <div id="submenu-2" class="collapse submenu">
                                    <ul class="nav flex-column">
                                        <li class="nav-item">
                                            <a class="nav-link" asp-controller="AdminProduct" asp-action="AddNewProduct"  style="color: coral">Add Product</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" asp-controller="AdminProduct" asp-action="ViewProduct"  style="color: coral">View Product</a>
                                        </li>
                                    </ul>
                                </div>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#" data-toggle="collapse" aria-expanded="false" data-target="#submenu-3" aria-controls="submenu-3"  style="color: coral"><i class="fas fa-fw fa fa-users"></i>Manage Users</a>
                                <div id="submenu-3" class="collapse submenu">
                                    <ul class="nav flex-column">
                                        <li class="nav-item">
                                            <a class="nav-link" asp-controller="AdminUser" asp-action="AddNewUser"  style="color: coral">Add User</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" asp-controller="AdminUser" asp-action="ViewUser"  style="color: coral">View User</a>
                                        </li>
                                    </ul>
                                </div>
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="#"  style="color: coral"><i class="fas fa-fw fa fa-pen-square" ></i>Edit Profile</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#"  style="color: coral"><i class="fas fa-fw fa fa-power-off"></i>Logout</a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- end left sidebar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        @RenderBody();
        <!-- ============================================================== -->
        <!-- footer -->
        <!-- ============================================================== -->
        @*<div class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                            Copyright © 2018 Concept. All rights reserved. Dashboard by <a href="https://colorlib.com/wp/">Colorlib</a>.
                        </div>
                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                            <div class="text-md-right footer-links d-none d-sm-block">
                                <a href="javascript: void(0);">About</a>
                                <a href="javascript: void(0);">Support</a>
                                <a href="javascript: void(0);">Contact Us</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>*@
        <!-- ============================================================== -->
        <!-- end footer -->
        <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- end wrapper  -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- end main wrapper  -->
    <!-- ============================================================== -->
    <!-- Optional JavaScript -->
    <!-- jquery 3.3.1 -->
    <script src="~/admin_js/jquery-3.3.1.min.js"></script>
    <!-- bootstap bundle js -->
    <script src="~/admin_js/bootstrap.bundle.js"></script>
    <!-- slimscroll js -->
    <script src="~/admin_js/jquery.slimscroll.js"></script>
    <!-- main js -->
    <script src="~/admin_js/main-js.js"></script>
    <!-- chart chartist js -->
    <script src="~/admin_js/chartist.min.js"></script>
    <!-- sparkline js -->
    <script src="~/admin_js/jquery.sparkline.js"></script>
    <!-- morris js -->
    <script src="~/admin_js/raphael.min.js"></script>
    <script src="~/admin_js/morris.js"></script>
    <!-- chart c3 js -->
    <script src="~/admin_js/c3.min.js"></script>
    <script src="~/admin_js/d3-5.4.0.min.js"></script>
    <script src="~/admin_js/C3chartjs.js"></script>
    <script src="~/admin_js/dashboard-ecommerce.js"></script>
</body>

</html>